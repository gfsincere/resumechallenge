apiVersion: v1
kind: ConfigMap
metadata:
  name: aws-auth
  namespace: kube-system
data:
  mapRoles: |
    - rolearn: arn:aws:iam::396913714027:role/EKS_ServiceAccount
      username: github_actions
      groups:
        - system:bootstrappers
        - system:nodes
    - rolearn: arn:aws:iam::396913714027:user/github_actions
      username: github_actions
      groups:
        - system:masters
  mapUsers: |
    - userarn: arn:aws:iam::396913714027:user/github_actions
      username: github_actions
      groups:
        - system:masters
